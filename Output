public class Output {

    public static void printResults(int[] resultsArray) {
        System.out.printf("You are " + resultsArray[0] + " percent like SpongeBob \n");
		System.out.printf("You are " + resultsArray[1] + " percent like Patrick \n");
		System.out.printf("You are " + resultsArray[2] + " percent like Squidward \n");
		System.out.printf("You are " +resultsArray[3] + " percent like Mr. Krabs \n");
		System.out.printf("You are " + resultsArray[4] + " percent like Sandy \n");
    }
    
}
