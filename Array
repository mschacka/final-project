package proj;

//Armando Bracco

public class Array {
 
 public static void getInputs(int[] quizAns) {
	 
	 quizAns[0] = proj.Input.scount;
	 quizAns[1] = proj.Input.pcount;
	 quizAns[2] = proj.Input.sqcount;
	 quizAns[3] = proj.Input.mcount;
	 quizAns[4] = proj.Input.sancount;
 }
 
 
 public static void convertToPercentage(int[] quizAns) {
	for(int i = 0; i < 5; i++) {
		int num = quizAns[i];
		quizAns[i] = num*100/7;
 }
 
 }	
}
